{
    "extends": "airbnb-base",
    "rules": {
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "no-console": 0,
        "import/no-unresolved": 0,
        "consistent-return": 0,
        "no-restricted-globals": 0,
        "no-param-reassign": 0,
        "radix": 0,
        "no-eval": 0,
        "prefer-destructuring": 0,
        "max-len": 0,
        "no-plusplus": 0,
        "no-restricted-syntax": 0,
        "no-loop-func": 0,
        "no-await-in-loop": 0,
        "no-else-return": 0
    }
}